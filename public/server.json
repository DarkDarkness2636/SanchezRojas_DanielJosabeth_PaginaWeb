{
    "cleanUrls": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/login",
            "destination": "/login.html"
        },
        {
            "source": "/register",
            "destination": "/register.html"
        },
        {
            "source": "/index",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/index.html",
            "destination": "/",
            "type": 301
        }
    ],
    "headers": [
        {
            "source": "**/*.@(html|htm)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate"
                },
                {
                    "key": "Content-Type",
                    "value": "text/html; charset=UTF-8"
                }
            ]
        }
    ]
}