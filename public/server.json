{
    "public": "public",
    "cleanUrls": false,
    "rewrites": [
        {
            "source": "/login",
            "destination": "/login.html"
        },
        {
            "source": "/register",
            "destination": "/register.html"
        },
        {
            "source": "**",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/login.html",
            "destination": "/login",
            "type": 301
        },
        {
            "source": "/register.html",
            "destination": "/register",
            "type": 301
        }
    ],
    "headers": [
        {
            "source": "**/*.html",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store"
                }
            ]
        }
    ]
}